{
  "name": "portfolio-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently --names \"VITE,CMS\" -c \"bgBlue.bold,bgMagenta.bold\" \"vite\" \"npx decap-server\"",
    "dev:vite": "vite",
    "dev:cms": "npx decap-server",
    "optimize": "node optimize-images.js",
    "prebuild": "node optimize-images.js",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "decap-cms-app": "~3.9.0",
    "heic-convert": "^2.1.0",
    "react": "~19.2.3",
    "react-dom": "~19.2.3",
    "react-router-dom": "~7.12.0"
  },
  "devDependencies": {
    "@eslint/js": "~9.39.2",
    "@types/node": "~24.10.4",
    "@types/react": "~19.2.7",
    "@types/react-dom": "~19.2.3",
    "@vitejs/plugin-react": "~5.1.2",
    "concurrently": "~9.2.1",
    "decap-server": "~3.5.0",
    "eslint": "~9.39.2",
    "eslint-plugin-react-hooks": "~7.0.1",
    "eslint-plugin-react-refresh": "~0.4.25",
    "globals": "~16.5.0",
    "npx-server": "~0.0.47",
    "sharp": "~0.34.5",
    "typescript": "~5.9.3",
    "typescript-eslint": "~8.50.0",
    "vite": "~7.3.0"
  }
}
